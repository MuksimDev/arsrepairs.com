}

.dev-keyword-research-recommendations li:last-child {
  margin-bottom: 0;
}

.dev-keyword-research-recommendations li.dev-keyword-research-recommendation--error {
  background: #fee2e2;
  color: #991b1b;
  border-left-color: #dc2626;
}

