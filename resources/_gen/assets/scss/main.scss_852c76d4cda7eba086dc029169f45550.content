}

@keyframes confetti-fall {
  0% {
    transform: translate(0, 0) rotate(0deg);
    opacity: 1;
  }
  50% {
    opacity: 0.5;
  }
  100% {
    transform: translate(var(--confetti-x, 0), var(--confetti-y, 0)) rotate(var(--confetti-rotate, 360deg));
    opacity: 0;
  }
}

